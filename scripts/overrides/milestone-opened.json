          "type": ["null"]
        },
        "closed_issues": {
          "type": "integer"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "creator": {
          "title": "User",
          "type": ["object", "null"],
          "properties": {
            "avatar_url": {
              "type": "string",
